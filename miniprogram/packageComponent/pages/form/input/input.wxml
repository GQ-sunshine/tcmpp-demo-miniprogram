      </view>
    </view>
    <view class="page-section">
      <view class="weui-cells__title">{{t['input14']}}</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <input class="weui-input" type="digit" placeholder="{{t['input15']}}" />
