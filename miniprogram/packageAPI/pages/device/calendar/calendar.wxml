<import src="../../../../common/head.wxml" />
<import src="../../../../common/foot.wxml" />
<import src="../../../../common/foot_en.wxml" />

<view class="container page">
  <template is="head" data="{{title: t['calendar0']}}"/>

  <view class="page-body">
    <view class="btn-area">
      <button type="primary" bindtap="addPhoneRepeatCalendar">addPhoneRepeatCalendar</button>
      <button type="primary" bindtap="addPhoneCalendar">addPhoneCalendar</button>
    </view>
  </view>

  <template is="foot" wx:if="{{lang==='zh_CN'}}" />
  <template is="foot_en" wx:else />
</view>
