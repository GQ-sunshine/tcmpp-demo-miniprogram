          <input class="weui-input" bindinput="bindHideKeyboard" placeholder="{{t['input9']}}" />
        </view>
      </view>
    </view>
    <view class="page-section">
      <view class="weui-cells__title">{{t['input10']}}</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <input class="weui-input" type="number" placeholder="{{t['input11']}}" />
        </view>
      </view>
    </view>
    <view class="page-section">
      <view class="weui-cells__title">{{t['input12']}}</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <input class="weui-input" password type="text" placeholder="{{t['input13']}}" />
